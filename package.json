{
  "name": "tic-tac-toe",
  "displayName": "tic-tac-toe",
  "description": "Simple game extension.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "tic-tac-toe.Tic-Tac-Toe",
        "displayName": "Tic Tac Toe",
        "selector": [
          {
            "filenamePattern": "*.ttt.json"
          }
        ],
        "priority": "option"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "@types/vscode": "^1.76.0",
    "typescript": "^4.9.5"
  }
}
