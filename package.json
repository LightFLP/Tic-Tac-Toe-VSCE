{
  "name": "my-webpack-project",
  "displayName": "tic-tac-toe",
  "description": "My webpack project",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "tic-tac-toe.Tic-Tac-Toe",
        "displayName": "Tic Tac Toe",
        "selector": [
          {
            "filenamePattern": "*.ttt.json"
          }
        ],
        "priority": "option"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile-extension",
    "compile-extension": "tsc -p ./",
    "compile-viewer": "npm run build:dev",
    "watch": "webpack --watch",
    "build": "webpack --mode=production --node-env=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "@types/vscode": "^1.76.0",
    "@webpack-cli/generators": "^3.0.1",
    "css-loader": "^6.7.3",
    "style-loader": "^3.3.2",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "webpack": "^5.76.2",
    "webpack-cli": "^5.0.1"
  }
}
